{
  "name": "fbl-church-app",
  "version": "0.0.0",
  "engines": {
    "node": ">=16.10.0 <=18.10.0",
    "npm": ">=8.3.1 <=9.6.3"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:external": "npm start -- --host 0.0.0.0 --configuration external",
    "start:production": "npm start -- --configuration local-production",
    "format": "prettier --config ./.prettierrc  \"src\" \"projects\" \"styles\"",
    "format:check": "npm run format -- --list-different",
    "format:fix": "npm run format -- --write",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "test": "ng test --code-coverage",
    "test:watch": "npm run test -- --watch",
    "test:sourcemaps": "npm run test -- --code-coverage --source-map=true",
    "test:sourcemaps:watch": "npm run test:sourcemaps -- --watch",
    "test:headless": "ng test --watch=false --code-coverage --source-map=true --browsers=ChromeHeadlessNoSandbox",
    "package:refresh": "rm package-lock.json && npm i",
    "build:production": "ng build --configuration production",
    "heroku-postbuild": "ng build --aot --configuration production",
    "pipeline:format": "npm run format -- --check",
    "pipeline:lint": "npm run lint",
    "pipeline:test": "npm run test:headless"
  },
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.12",
    "@angular/cli": "^16.2.13",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@auth0/angular-jwt": "^5.0.2",
    "@fortawesome/angular-fontawesome": "^0.13.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@ng-select/ng-select": "11.2.0",
    "@stomp/rx-stomp": "1.2.0",
    "angular-bootstrap-checkbox": "^0.5.0",
    "angular-calendar": "0.31.1",
    "angularx-qrcode": "^16.0.2",
    "bootstrap": "^5.1.3",
    "calendar-utils": "0.10.4",
    "compression": "^1.7.4",
    "core-js": "^3.23.4",
    "date-fns": "^3.6.0",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "highcharts": "^11.4.3",
    "jquery": "^3.5.1",
    "lodash-es": "^4.17.21",
    "ng2-currency-mask": "^13.0.3",
    "ng2-file-upload": "5.0.0",
    "ngx-colors": "^3.6.0",
    "ngx-toastr": "^17.0.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.0.0",
    "zone.js": "0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.13",
    "@angular-eslint/builder": "16.3.1",
    "@angular-eslint/eslint-plugin": "16.3.1",
    "@angular-eslint/eslint-plugin-template": "16.3.1",
    "@angular-eslint/schematics": "16.3.1",
    "@angular-eslint/template-parser": "16.3.1",
    "@angular/language-service": "^16.2.12",
    "@types/bootstrap": "^4.5.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~3.8.0",
    "@types/jquery": "^3.3.38",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "prettier": "^2.6.1",
    "ts-node": "~7.0.0",
    "typescript": "5.0.3"
  }
}
