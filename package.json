{
  "name": "fb-awana-app",
  "version": "0.0.0",
  "engines": {
    "node": ">=16.10.0 <17.0.0",
    "npm": ">=8.3.1"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build:production": "ng build --configuration production",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "test": "ng test --code-coverage --source-map=false",
    "test:watch": "ng test --code-coverage --source-map=false --watch",
    "test:sourcemaps": "ng test --code-coverage --source-map=true",
    "test:sourcemaps:watch": "ng test --code-coverage --source-map=true --watch",
    "test:headless": "ng test --watch=false --code-coverage --source-map=true --browsers=ChromeHeadless",
    "serve": "ng serve",
    "serve:external": "ng serve --host 0.0.0.0",
    "serve:production": "ng serve --configuration production",
    "package:refresh": "rm package-lock.json && npm i",
    "heroku-postbuild": "ng build --configuration production"
  },
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "@auth0/angular-jwt": "^5.0.2",
    "@fortawesome/angular-fontawesome": "^0.11.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@stomp/rx-stomp": "1.2.0",
    "angular-bootstrap-checkbox": "^0.5.0",
    "angular-calendar": "^0.28.28",
    "bootstrap": "^5.1.3",
    "core-js": "^3.23.4",
    "date-fns": "^2.28.0",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "jquery": "^3.5.1",
    "ngx-toastr": "^13.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/builder": "14.4.0",
