{
  "name": "fbl-church-app",
  "version": "0.0.0",
  "engines": {
    "node": ">=16.10.0 <17.0.0",
    "npm": ">=8.3.1 <=9.6.3"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build:production": "ng build --configuration production",
    "format": "prettier --config ./.prettierrc  \"src\" \"projects\" \"styles\"",
    "format:check": "npm run format -- --list-different",
    "format:fix": "npm run format -- --write",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "test": "ng test --code-coverage",
    "test:watch": "npm run test -- --watch",
    "test:sourcemaps": "npm run test -- --code-coverage --source-map=true",
    "test:sourcemaps:watch": "npm run test:sourcemaps -- --watch",
    "test:headless": "ng test --watch=false --code-coverage --source-map=true --browsers=ChromeHeadlessNoSandbox",
    "serve": "ng serve",
    "serve:external": "ng serve --host 0.0.0.0 --configuration external",
    "serve:production": "ng serve --configuration production",
    "package:refresh": "rm package-lock.json && npm i",
    "heroku-postbuild": "ng build --configuration production && gzipper --verbose ./dist",
    "pipeline:format": "npm run format -- --check",
    "pipeline:lint": "npm run lint",
    "pipeline:test": "npm run test:headless"
  },
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "@auth0/angular-jwt": "^5.0.2",
    "@fortawesome/angular-fontawesome": "^0.11.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@ng-select/ng-select": "^9.0.2",
    "@stomp/rx-stomp": "1.2.0",
    "angular-bootstrap-checkbox": "^0.5.0",
    "angular-calendar": "^0.28.28",
    "angularx-qrcode": "^14.0.0",
    "bootstrap": "^5.1.3",
    "core-js": "^3.23.4",
    "date-fns": "^2.28.0",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "jquery": "^3.5.1",
    "ng2-file-upload": "1.4.0",
    "ngx-toastr": "^13.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "^14.2.10",
    "@angular/compiler-cli": "^14.2.12",
    "@angular/language-service": "^14.2.12",
    "@types/bootstrap": "^4.5.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~3.8.0",
    "@types/jquery": "^3.3.38",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "gzipper": "^7.2.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "prettier": "^2.6.1",
    "ts-node": "~7.0.0",
    "typescript": "~4.6.4"
  }
}